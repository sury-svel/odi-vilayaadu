{
  "expo": {
    "name": "odi-vilayaadu",
    "slug": "expo-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "odiVilayaadu",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.amchats.odivilayaadu",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": ["odiVilayaadu"]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "org.amchats.odivilayaadu",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "odiVilayaadu",
              "host": "*",
              "pathPrefix": "/"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "googleServicesFile": "./credentials/google-services.json"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-localization",
      "expo-notifications",
      ["expo-build-properties", {
        "android": { "useNextNotificationsApi": true }
      }]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "fcfb2208-51f8-4004-9232-52ccb4eb262c"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/fcfb2208-51f8-4004-9232-52ccb4eb262c"
    },
    "owner": "suryans"
  }
}
